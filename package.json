{
    "name": "duty_calculator",
    "version": "1.0.1",
    "description": "",
    "main": "server.js",
    "scripts": {
        "start": "node server.js",
        "prod": "node server.js",
        "dev": "nodemon server.js",
        "devs": "nodemon https.js",
        "ssl": "openssl req -x509 -nodes -days 365 -newkey rsa:2048 -keyout ./security/cert.key -out ./security/cert.pem -config ./security/req.cnf -sha256",
        "live": "ngrok http http://localhost:5555",
        "test": "cross-env NODE_ENV=test jest --testTimeout=10000"
    },
    "author": "",
    "dependencies": {
        "bcryptjs": "^2.4.3",
        "body-parser": "^1.20.0",
        "compression": "^1.7.4",
        "cors": "^2.8.5",
        "dotenv": "^16.0.2",
        "express": "^4.18.1",
        "express-sequelize-logger": "github:prateekm03/express-sequelize-logger",
        "i18next": "^21.9.2",
        "i18next-fs-backend": "^1.1.5",
        "i18next-http-middleware": "^3.2.1",
        "i18next-sprintf-postprocessor": "^0.2.2",
        "jsonwebtoken": "^8.5.1",
        "mysql2": "^2.3.3",
        "request": "^2.88.2",
        "sequelize": "^6.23.2",
        "ua-parser-js": "^1.0.2",
        "uuid": "^9.0.0"
    },
    "nodemonConfig": {
        "restartable": "rs",
        "ext": "js,html",
        "watch": [
            "app",
            ".env",
            "app.js",
            "server.js"
        ],
        "ignore": [
            ".git",
            ".idea",
            "build"
        ],
        "execMap": {
            "js": "node --harmony"
        },
        "env": {
            "NODE_ENV": "development"
        },
        "verbose": true,
        "stdout": true,
        "stderr": true,
        "quiet": false,
        "cwd": ".",
        "watchDelay": 2000,
        "debug": false,
        "logLevel": "info",
        "logFile": "nodemon.log",
        "exec": "node --harmony",
        "nodeArgs": [
            "--harmony",
            "--trace-warnings"
        ],
        "ignoreHidden": true,
        "delay": 2500,
        "legacyWatch": true,
        "exitcrash": true,
        "cleanupDelay": 0
    },
    "devDependencies": {
        "nodemon": "^2.0.20"
    }
}
